#/* vim: set filetype=sh : */

if [ ! -f ~/.fzf.zsh ]; then
  echo "Performing initial setup of fzf"
  # We install without shell setup as we do this in our individual shells
  git clone --depth 1 https://github.com/junegunn/fzf.git ~/.fzf && ~/.fzf/install
fi

if [ -f ~/.local_shellrc ]; then
    source ~/.local_shellrc
fi

source ~/.config/shell/alias.sh
source ~/.config/shell/util.bash
source ~/.config/shell/task.bash
source ~/.config/shell/env.sh
source ~/.config/shell/ros.bash
source ~/.config/shell/completions/ros_fzf_completions.sh
source ~/.config/shell/completions/git_fzf_completions.sh
